<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxWidth="170.0" prefWidth="170.0" style="-fx-background-color: #303F9F;" stylesheets="@../styles/ArgusDoc.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DrawerMainMenuController">
   <children>
      <Pane prefHeight="100.0" prefWidth="170.0" />
      <ImageView fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/drawlerLog.jpg" />
         </image>
      </ImageView>
      <Pane prefHeight="40.0" prefWidth="170.0" />
      <JFXButton prefHeight="30.0" prefWidth="170.0" styleClass="JFXButton" text="Смена пользователя" />
      <JFXButton prefHeight="30.0" prefWidth="170.0" styleClass="JFXButton" text="О программе" />
      <JFXButton fx:id="exitProgram" onAction="#exitProgram" prefHeight="30.0" prefWidth="170.0" styleClass="JFXButton" text="Выход из программы" />
   </children>
</VBox>
